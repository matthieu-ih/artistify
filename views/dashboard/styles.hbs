<h1>All styles</h1>

<table>
    <tr>    
        <td>color</td>
        <td>name</td>
        <td>wiki</td>
        <td>update</td>
        <td>delete</td>
    </tr>
{{#each styles}}
    <tbody>
        <tr>
            <td style="background-color:{{this.color}};"></td>
            <td>{{this.name}}</td>
            <td><a href="{{this.wikiURL}}">link</a></td>
            <td><a href="/dashboard/style/update/{{this.id}}">update</a></td>
            <td><a href="/">delete</a></td>
        </tr>
    </tbody>

{{/each}}
</table>
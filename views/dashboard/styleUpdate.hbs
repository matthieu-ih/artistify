<form method="post" action="/dashboard/style/update/{{style.id}}">

    <label for="name">Name</label>
    <input  type="text" name="name" id="name" value="{{style.name}}">

    <label for="color">Color</label>
    <input type="color" name="color" id="color" value="{{style.color}}">

    <label for="wikiURL">wikiURL</label>
    <input type="text" name="wikiURL" id="wikiURL" value="{{style.wikiURL}}">

    <button>Send</button>

</form>